EMPLOYEE TABLE
SELECT Emp_Type, COUNT(*) AS Emp_Count
FROM EMPLOYEE
GROUP BY Emp_Type
HAVING COUNT(*) > 5;

CUSTOMER TABLE
SELECT Cust_Num, COUNT(*) AS Cust_Count
FROM CUSTOMER
GROUP BY Cust_Num
HAVING COUNT(*) > 2;

BOOKING
SELECT Table_Num, COUNT(*) AS Booking_Count
FROM BOOKINGS
GROUP BY Table_Num
HAVING COUNT(*) > 3;
